{
  "name": "@thlorenz/electron-mksnapshot",
  "version": "0.0.0",
  "description": "Configurable electron version of the mksnapshot binary",
  "repository": "git@github.com:thlorenz/mksnapshot.git",
  "main": "dist/mksnapshot.js",
  "scripts": {
    "env": "env | grep npm",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./dist",
    "lint": "prettier -c ./src/",
    "lint:fix": "prettier --format ./src",
    "watch": "tsc -p ./tsconfig.json --watch",
    "mksnapshot": "node ./dist/mksnapshot.js",
    "_install": "node ./dist/download-mksnapshot.js",
    "test": "mocha && standard"
  },
  "dependencies": {
    "@electron/get": "^1.12.4",
    "debug": "^4.3.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "supports-color": "^9.0.1",
    "temp": "^0.9.4",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.12.0",
    "@types/temp": "^0.9.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "author": "Thorsten Lorenz <thlorenz@gmx.de>",
  "license": "MIT",
  "private": false
}
